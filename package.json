{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy microfinance",
        "canister_deploy_mainnet": "dfx deploy --network ic microfinance",
        "canister_uninstall": "dfx canister uninstall-code microfinance",
        "canister_call_get_message": "dfx canister call microfinance getMessage",
        "canister_call_set_message": "dfx canister call microfinance setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "axios": "^1.6.2",
        "azle": "0.18.6"
    }
}
